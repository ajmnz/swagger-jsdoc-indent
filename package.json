{
  "name": "swagger-jsdoc-indent",
  "displayName": "swagger-jsdoc comment formatter",
  "description": "Write indentable OpenApi/Swagger jsDoc comments",
  "publisher": "ajmnz",
  "version": "0.0.1",
  "repository": {
    "url": "https://github.com/ajmnz/swagger-jsdoc-indent"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onCommand:swagger-jsdoc-indent.format"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "swagger-jsdoc-indent.format",
        "title": "Format swagger-jsdoc comment"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "package": "yarn clean && tsc -p",
    "clean": "rimraf -rf ./dist",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/prettier": "^2.3.2",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "prettier": "^2.3.2"
  }
}
